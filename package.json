{
  "name": "autoadmin",
  "version": "0.0.1",
  "description": "管理后台自动化生成项目模版",
  "main": "app.js",
  "scripts": {
    "dev": "node server.js",
    "build": "babel ./src -d ./dist",
    "router": "node generate-router.js",
    "deploy": "export NODE_ENV=production && node sync-version.js && rm -rf build/assets && webpack --config ./webpack.config.js --progress --profile --colors && cp -R build/* ../admin_pro/",
    "deployforwindows": "set NODE_ENV=production && node sync-version.js && webpack --config ./webpack.config.js --progress --profile --colors"
  },
  "author": "hgwang",
  "dependencies": {
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0"
  },
  "devDependencies": {
    "antd": "^2.8.2",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.24.1",
    "image-loader": "0.0.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "moment": "^2.17.1",
    "node-sass": "^4.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.3.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1"
  },
  "clean": {
    "react": [
      "node_modules/**/react",
      "!node_modules/react"
    ]
  },
  "repository": {},
  "license": "MIT"
}
